<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
<route url="/V1/vehicleapi/car" method="GET">
    <service class="Vehicles\Cars\Api\DemoInterface" method="getProfile"/>
    <resources>
        <resource ref="anonymous"/>
    </resources>
</route>
<route url="/V1/vehicleapi/couper" method="POST">
    <service class="Vehicles\Cars\Api\DemoInterface" method="setCustomData"/>
    <resources>
        <resource ref="anonymous"/>
    </resources>
</route>
</routes>

